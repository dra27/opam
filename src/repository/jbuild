#use "../jbuild.common"

let () = out {|
(jbuild_version 1)

(library
  ((name opam_repository)
   (public_name opam-repository)
   (synopsis "OCaml Package Manager remote repository handling library")
   (libraries (opam-format))
   (flags (:standard %s))
   (wrapped false)))
|} ocaml_flags
